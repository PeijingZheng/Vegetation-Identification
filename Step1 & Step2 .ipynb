    "import gdal\n",
    "import os\n",
    "import random\n",
    "\n",
    "#1.Read tif dataset\n",
    "def readTif(fileName):\n",
    "    dataset = gdal.Open(fileName)\n",
    "    if dataset == None:\n",
    "        print(fileName+\"File cannot be opened\")\n",
    "    return dataset\n",
    "\n",
    "\n",
    "Landset_Path = r\"C:\\Users\\admin\\Desktop\\orginal tif\\2020-07-27-00_00_2020-07-27-23_59_Sentinel-2_L1C_True_color.tif\"\n",
    "LabelPath = r\"C:\\Users\\admin\\Desktop\\label\\2020-bjql1.tif\"\n",
    "txt_Path = r\"D:\\data.txt\"\n",
    "\n",
    "#2.Read image data\n",
    "dataset = readTif(Landset_Path)\n",
    "Tif_width = dataset.RasterXSize #Number of columns in the raster matrix\n",
    "Tif_height = dataset.RasterYSize #Number of rows of the raster matrix\n",
    "Tif_bands = dataset.RasterCount #Number of bands\n",
    "Tif_geotrans = dataset.GetGeoTransform()#Get affine matrix information\n",
    "Landset_data = dataset.ReadAsArray(0,0,Tif_width,Tif_height)\n",
    "dataset = readTif(LabelPath)\n",
    "Label_data = dataset.ReadAsArray(0,0,Tif_width,Tif_height)\n",
    "\n",
    "\n",
    "# 3.Before writing, check if the file exists, and delete it if it does.\n",
    "if os.path.exists(txt_Path):\n",
    "    os.remove(txt_Path)\n",
    "# Opens a file as a write and automatically creates the file if it doesn't exist\n",
    "file_write_obj = open(txt_Path, 'w')\n",
    "\n",
    "#3.Collection of vegetation category samples,\n",
    "#Iterate through all pixel values,\n",
    "#Collect all the image elements for the vegetation.\n",
    "count = 0\n",
    "for i in range(Label_data.shape[0]):\n",
    "    for j in range(Label_data.shape[1]):\n",
    "        #  I set the vegetation category to have an image metric value of 1 in the labeled map\n",
    "        if(Label_data[i][j] == 1):\n",
    "            var = \"\"\n",
    "            for k in range(Landset_data.shape[0]):\n",
    "                var = var + str(Landset_data[k][i][j])+\",\"\n",
    "            var = var + \"Vegetation\"\n",
    "            file_write_obj.writelines(var)\n",
    "            file_write_obj.write('\\n')\n",
    "            count = count + 1\n",
    "   \n",
    "#4.Collecting samples from non-vegetation categories，\n",
    "#Because there are many more non-vegetated samples than vegetated samples，\n",
    "#So using a random selection of non-vegetation samples in all non-vegetation categories，\n",
    "#The number is consistent with the number of vegetation samples.\n",
    "Threshold = count\n",
    "count = 0\n",
    "#Still traversing all pixel values.\n",
    "for i in range(Label_data.shape[0]):\n",
    "    for j in range(Label_data.shape[1]):\n",
    "        # I set the non-vegetation category to have an image element value of 23 in the labeled map\n",
    "        if(Label_data[i][j] == 23):\n",
    "            var = \"\"\n",
    "            for k in range(Landset_data.shape[0]):\n",
    "                var = var + str(Landset_data[k][i][j])+\",\"\n",
    "            var = var + \"Non-Vegetation\"\n",
    "            file_write_obj.writelines(var)\n",
    "            file_write_obj.write('\\n')\n",
    "            count = count + 1\n",
    "        if(count == Threshold):\n",
    "            break\n",
    "\n",
    "file_write_obj.close()"
