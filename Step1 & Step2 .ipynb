    "from sklearn.ensemble import RandomForestClassifier\n",
    "import numpy as np\n",
    "from sklearn import model_selection\n",
    "import pickle \n",
    "\n",
    "#  Define dictionaries to facilitate parsing of the sample dataset.txt\n",
    "def Iris_label(s):\n",
    "    it={b'Vegetation':0, b'Non-Vegetation':1}\n",
    "    return it[s]\n",
    "\n",
    "path=r\"E:\\all.txt\"\n",
    "SavePath = r\"D:\\model.pickle\"\n",
    "\n",
    "\n",
    "#  1.Reading a dataset\n",
    "print(\"Start loading data....\")\n",
    "data=np.loadtxt(path, dtype=float, delimiter=',', converters={3:Iris_label} )   # 改为4\n",
    "print(\"Data loaded successfully.....   \",data.shape)\n",
    "#  converters={7:Iris_label},where "7" refers to column 8: convert str in column 8 to label(number)\n",
    "\n",
    "#  2.Divide the data with labels\n",
    "x,y=np.split(data,indices_or_sections=(3,),axis=1) #x is the data and y is the label\n",
    "x=x[:,0:3] #The three bands were selected to characterize\n",
    "train_data,test_data,train_label,test_label = model_selection.train_test_split(x,y, random_state=1, train_size=0.9,test_size=0.1)\n",
    "print(\"Successful division of training data and validation data.....\" )\n",
    "\n",
    "#  3.Creating a random forest model with 100 trees to train a random forest\n",
    "print(\"Start training.....\" )\n",
    "classifier = RandomForestClassifier(n_estimators=100, \n",
    "                               bootstrap = True,\n",
    "                               max_features = 'sqrt')\n",
    "classifier.fit(train_data, train_label.ravel())#The ravel function stretches to one dimension\n",
    "\n",
    "\n",
    "#  4.Calculating the accuracy of random forests\n",
    "print(\"training set：\",classifier.score(train_data,train_label))\n",
    "print(\"testing set：\",classifier.score(test_data,test_label))\n",
    "\n",
    "#  5.Saving Models\n",
    "#Open the file as a binary：\n",
    "file = open(SavePath, \"wb\")\n",
    "#Write the model to a file：\n",
    "pickle.dump(classifier, file)\n",
    "#Final closure of the file：\n",
    "file.close()"
   ]
  },
